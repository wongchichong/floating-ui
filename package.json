{
  "name": "@floating-ui/monorepo",
  "packageManager": "pnpm@10.15.0",
  "private": true,
  "version": "0.0.0",
  "description": "Floating UI monorepo",
  "engines": {
    "node": ">=18.19.0",
    "pnpm": ">=8.13.1"
  },
  "workspaces": [
    "./packages/*",
    "extension",
    "config"
  ],
  "scripts": {
    "lint": "turbo lint",
    "format": "turbo format",
    "playwright": "turbo playwright",
    "playwright:update": "turbo playwright -- -u",
    "build:turbo": "turbo build",
    "build:api": "turbo build:api",
    "build:utils": "cd packages/utils && pnpm i && pnpm build",
    "build:dom": "cd packages/dom && pnpm i && pnpm build",
    "build:core": "cd packages/core && pnpm i && pnpm build",
    "build:woby-dom": "cd packages/woby-dom && pnpm i && pnpm build",
    "build:woby": "cd packages/woby && pnpm i && pnpm build",
    "build": "pnpm build:utils && pnpm build:dom && pnpm build:core && pnpm build:woby-dom",
    "clean": "turbo clean",
    "publint": "turbo publint",
    "test": "turbo test",
    "prepack": "turbo prepack",
    "typecheck": "turbo typecheck",
    "release": "turbo prepack && turbo publint && changeset publish"
  },
  "author": "",
  "license": "MIT",
  "browserslist": [
    "Chrome >= 73",
    "Firefox >= 78",
    "Edge >= 79",
    "Safari >= 12.0",
    "iOS >= 12.0",
    "Opera >= 53"
  ],
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "useTabs": false,
    "semi": true
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@changesets/cli": "^2.29.5",
    "@changesets/types": "^6.1.0",
    "@eslint/js": "^9.32.0",
    "@microsoft/api-extractor": "^7.52.10",
    "@playwright/test": "^1.54.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@testing-library/jest-dom": "^6.6.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@vitest/browser": "^3.2.4",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-copy": "^3.5.0",
    "tailwindcss": "^4.1.11",
    "turbo": "^2.5.5",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  }
}